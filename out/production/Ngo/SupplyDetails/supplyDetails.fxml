<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SupplyDetails.SupplyDetailsController">
   <children>
      <Pane fx:id="supplyDetailsPane" prefHeight="1275.0" prefWidth="1473.0">
         <children>
            <Label layoutX="73.0" layoutY="15.0" prefHeight="98.0" prefWidth="511.0" text="Supply Details">
               <font>
                  <Font name="Californian FB" size="36.0" />
               </font>
            </Label>
            <HBox alignment="CENTER" layoutX="200.0" layoutY="350.0" prefHeight="280.0" prefWidth="1000.0" spacing="80.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#handleSupplyFood" onMouseClicked="#handleSupplyFood" prefHeight="210.0" prefWidth="210.0" text="Food">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#handleSupplyMedicine" prefHeight="210.0" prefWidth="210.0" text="Medicine">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Button>
                  <Button mnemonicParsing="false" onAction="#handleSupplyEmergency" prefHeight="210.0" prefWidth="210.0" text="Emergency">
                     <font>
                        <Font size="24.0" />
                     </font>
                  </Button>
               </children>
               <padding>
                  <Insets bottom="50.0" left="100.0" right="100.0" top="50.0" />
               </padding>
            </HBox>
         </children>
      </Pane>
   </children>
</AnchorPane>
